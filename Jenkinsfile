pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                // Build the application
                sh 'echo build'
            }
        }
        stage('Test') {
            steps {
                // Run the tests
                sh 'echo test'
            }
        }
        stage('Deploy') {
            steps {
                // Deploy the application
                sh 'echo deploy'
            }
        }
    }
}
